<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="de.leo.android.testfeature.MainActivity">

    <Button
        android:id="@+id/btnNotifyMaterialShareIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notification Share (Material)"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btnNotifyMaterialCheckboxIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notification Checkbox Black (Material)"
        android:layout_below="@+id/btnNotifyMaterialShareIcon"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btnNotifyLeo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notification Leo (Imported from Inkscape)"
        android:layout_below="@+id/btnNotifyMaterialCheckboxIcon"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
